root ::= parseddata
VehicleType ::= "\"car\"" | "\"bus\"" | "\"motorcycle\"" | "\"bicycle\"" | "\"pedestrian\"" | "\"truck\"" | "\"ev\""
parseddata ::= "{"   ws   "\"startlocation\":"   ws   string   ","   ws   "\"endlocation\":"   ws   string   ","   ws   "\"vehicletype\":"   ws   VehicleType   ","   ws   "\"visionImpairment\":"   ws   boolean   ","   ws   "\"mobilityPhysicalImpairment\":"   ws   boolean   ","   ws   "\"hearingImpairment\":"   ws   boolean   ","   ws   "\"breathingIssues\":"   ws   boolean   ","   ws   "\"dyslexia\":"   ws   boolean   "}"
parseddatalist ::= "[]" | "["   ws   parseddata   (","   ws   parseddata)*   "]"
string ::= "\""   ([^"]*)   "\""
boolean ::= "true" | "false"
ws ::= [ \t\n]*
number ::= [0-9]+   "."?   [0-9]*
stringlist ::= "["   ws   "]" | "["   ws   string   (","   ws   string)*   ws   "]"
numberlist ::= "["   ws   "]" | "["   ws   string   (","   ws   number)*   ws   "]"
